{
  "name": "plaisio/company-resolver-uni",
  "description": "PhpPlaisio: Uni Company Resolver",
  "keywords": [
    "PhpPlaisio",
    "Uni",
    "Company",
    "Resolver"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "plaisio/company-resolver": "^2.2.0"
  },
  "require-dev": {
    "ext-dom": "*",
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.22",
    "setbased/error-handler": "1.3.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\CompanyResolver\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\CompanyResolver\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
