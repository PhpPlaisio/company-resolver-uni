{
  "name": "plaisio/company-resolver-uni",
  "description": "PhpPlaisio: Uni Company Resolver",
  "keywords": [
    "PhpPlaisio",
    "Uni",
    "Company",
    "Resolver"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "plaisio/company-resolver": "^1.1"
  },
  "require-dev": {
    "phing/phing": "^2.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\CompanyResolver\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\CompanyResolver\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
